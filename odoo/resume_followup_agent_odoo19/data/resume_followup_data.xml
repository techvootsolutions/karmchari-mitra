<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Default Agent Settings -->
        <record id="default_agent_settings" model="resume.agent.settings">
            <field name="agent_name">Techvootbot</field>
            <field name="company_name">Techvoot Solution</field>
            <field name="default_job_title">WordPress Developer</field>
            <field name="conversation_mode">text</field>
            <field name="temperature">0.7</field>
            <field name="voice_gender">female</field>
            <field name="audio_timeout">5</field>
            <field name="silence_threshold">400</field>
            <field name="max_conversation_length">50</field>
            <field name="response_timeout">30</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
